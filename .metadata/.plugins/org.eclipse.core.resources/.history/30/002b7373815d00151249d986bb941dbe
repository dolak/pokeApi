var buildUi = function(pokemonDex){
	console.log("UI");
	
	var win = Ti.UI.createWindow({
		backgroundColor : "#8CC9D9"
	});
	
	var name = Ti.UI.createLabel({
		text : pokemonDex.name,
		color: "#232323",
		font: {
			fontSize: 40
		},
		top: 50
		
	});
	
		var type1 = Ti.UI.createLabel({
		text : pokemonDex.type1,
		color: "#232323",
		font: {
			fontSize: 40
		},
		top: 50, left: 10
		
	});

	win.add(name);
	win.open();
};

exports.buildUi = buildUi;