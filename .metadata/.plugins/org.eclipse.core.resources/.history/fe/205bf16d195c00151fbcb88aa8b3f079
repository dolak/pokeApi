exports.getData = function() {
	console.log("get data works.");

	var url = "http://pokeapi.co/api/v1/";
	var client = Ti.Network.createHTTPClient({
		onload : function(e) {
			var json = JSON.parse(this.responseText);
			console.log(json);

			var redditObject = {
				title : json.data.children[i].data.title,
				author : json.data.children[i].data.author,
				image : json.data.children[i].data.preview.images[0].resolutions[0].url,
			};
			redditArray.push(redditObject);

			var save = require("database");
			save.dataSave(redditArray);

			remoteDB.cloudSave(redditArray);

			console.log("reddit data works");
			console.log(redditArray);
		},

		onerror : function(e) {
			Ti.API.debug(e.error);
			alert("error");
		},
		timeout : 5000
	});

	client.open("GET", url);
	client.send();

};
